@model IEnumerable<MyUser>
@{
    ViewData["Title"] = "User List";
}
<h1>Blogger </h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>UserName</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            if(item.Name != "Sys")
            {
                <tr>
                    <td> @item.Id</td>
                    <td> @item.Name</td>
                    <td> @item.UserName</td>
                </tr>
            }
           
        }
    </tbody>
</table>

